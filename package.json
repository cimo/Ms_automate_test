{
    "name": "ms_automate_test",
    "version": "1.0.0",
    "description": "Microservice automate frontend test.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/Ms_automate_test.git"
    },
    "main": "./src/Controller/Server.ts",
    "scripts": {
        "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx --yes npm-force-resolutions@0.0.3",
        "check": "eslint . --fix",
        "execute": "npm run check && nodemon --watch src --ext ts --exec 'tsc && node ./dist/Controller/Server.js'",
        "build": "npm run check && tsc --build -clean && tsc --build"
    },
    "dependencies": {
        "express": "4.18.1",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5"
    },
    "devDependencies": {
        "typescript": "4.9.4",
        "nodemon": "2.0.21",
        "eslint": "8.35.0",
        "prettier": "2.8.7",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-config-prettier": "8.8.0",
        "@types/node": "18.14.4",
        "@typescript-eslint/parser": "5.54.0",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@types/express": "4.17.17",
        "@types/cookie-parser": "1.4.3",
        "@types/cors": "2.8.13"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7"
    }
}
