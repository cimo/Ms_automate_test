{
    "name": "ms_automate_test",
    "version": "1.0.0",
    "description": "Microservice automate frontend test.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/Ms_automate_test.git"
    },
    "scripts": {
        "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
        "sass": "sass --watch ./src/style/main.scss ./public/style/main.css",
        "check": "eslint . --fix",
        "execute": "nodemon --config nodemon.json",
        "build": "npm run check && tsc --build -clean && tsc --build tsconfig.build.json"
    },
    "dependencies": {
        "@cimo/form-data_parser": "1.1.7",
        "@cimo/websocket": "file:cimo-websocket-1.0.0.tgz",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5",
        "express": "4.18.1",
        "twing": "6.0.0-alpha.1",
        "playwright": "latest",
        "@playwright/test": "latest"
    },
    "devDependencies": {
        "@types/node": "18.14.4",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "@types/express": "4.17.17",
        "@types/cookie-parser": "1.4.3",
        "@types/cors": "2.8.13",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-prettier": "4.2.1",
        "prettier": "2.8.7",
        "prettier-plugin-twig-melody": "0.4.6",
        "sass": "1.57.1",
        "typescript": "4.9.4",
        "nodemon": "2.0.21"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7"
    }
}
