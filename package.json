{
    "name": "ms_automate_test",
    "version": "1.0.0",
    "description": "Microservice automate test.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/Ms_automate_test.git"
    },
    "scripts": {
        "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
        "remove_dist": "node -e \"const fs = require('fs'); try{ fs.rmdirSync('./dist/', { recursive: true }) } catch{}; process.exit(0);\"",
        "remove_public_js": "node -e \"const fs = require('fs'); try{ fs.rmdirSync('./public/js/', { recursive: true }) } catch{}; process.exit(0);\"",
        "sass": "sass ./src/style/main.scss ./public/style/main.css",
        "check": "eslint --fix",
        "build-server": "npm run remove_dist && npm run check && tsc --build tsconfig.build.json",
        "build-app": "npm run remove_public_js && npm run sass && webpack --config webpack.build.js",
        "build": "npm run build-server && npm run build-app",
        "execute": "nodemon --config nodemon.json",
        "execute-ui": "npx playwright test --ui --config=./src/playwright.config.ts"
    },
    "dependencies": {
        "@cimo/authentication": "1.1.11",
        "@cimo/pid": "1.0.5",
        "@cimo/websocket": "1.1.34",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5",
        "express": "4.19.0",
        "express-rate-limit": "7.2.0",
        "playwright": "1.39.0",
        "@playwright/test": "1.39.0",
        "twing": "6.0.0-alpha.1",
        "material-components-web": "14.0.0"
    },
    "devDependencies": {
        "@types/node": "18.14.4",
        "@types/express": "4.17.17",
        "@types/cookie-parser": "1.4.3",
        "@types/cors": "2.8.13",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "eslint": "8.35.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-config-prettier": "8.8.0",
        "eslint-webpack-plugin": "4.0.1",
        "prettier": "2.8.7",
        "prettier-plugin-twig-melody": "0.4.6",
        "typescript": "4.9.4",
        "sass": "1.57.1",
        "nodemon": "2.0.21",
        "webpack": "5.79.0",
        "webpack-cli": "5.0.1",
        "terser-webpack-plugin": "5.3.9",
        "compression-webpack-plugin": "10.0.0",
        "html-webpack-plugin": "5.6.0",
        "ts-loader": "9.4.2",
        "dotenv": "16.0.3"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7",
        "crypto-js": "4.2.0",
        "**/crypto-js": "4.2.0"
    }
}
